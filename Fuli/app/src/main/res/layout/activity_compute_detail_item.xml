<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="37dp"
    android:gravity="center_vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@mipmap/compute_detail_listitem_bg"
        android:gravity="center_vertical">

        <TextView
            android:id="@+id/computeDetailItemTimeDay"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:lines="1"
            android:text="2016-05-07"
            android:textColor="@color/colorText" />

        <TextView
            android:id="@+id/computeDetailItemTimeSecond"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:lines="1"
            android:text="18:36:16:216"
            android:textColor="@color/colorText" />

        <TextView
            android:id="@+id/computeDetailItemNumber"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:lines="1"
            android:text="10004856"
            android:textColor="@color/colorTopGuide" />

        <TextView
            android:id="@+id/computeDetailItemName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:lines="1"
            android:text="自强不息八方"
            android:textColor="@color/colorText" />
    </LinearLayout>

</RelativeLayout>
